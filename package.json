    "react": "18.0.0",
    "react-native": "0.69.4",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-reanimated": "^2.11.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.17.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "ts-jest": "^26.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
